@model WebApplication3.Models.Operaciones

<!-- Modal -->
@*<div class="modal fade" id="modalOperaciones" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">*@
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Datos Operación</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <p>Código operación: @Model.Operacion_PK</p>
                <p>Códig moneda: @Model.Moneda_PK</p>
                <p>Código Tipo Operación: @Model.TipoOperacion_PK</p>
                <p>Fecha operación: @Model.FechaOperacion</p>
                <p>Importe: @Model.Importe</p>
                <p>Num Monedas: @Model.NumMonedas</p>
                <p>Fecha creación: @Model.FechaCreacion</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal" id="btnClose">Close</button>
            </div>
        </div>
    </div>
@*</div>*@
